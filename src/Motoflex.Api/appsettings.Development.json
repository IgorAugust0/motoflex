{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "User ID=postgres;Password=123456;Host=localhost;Port=5432;Database=postgres;Pooling=true;"
  },
  "AWS": {
    "AccessKey": "your-access-key",
    "SecretKey": "your-secret-key",
    "BucketName": "aplicacao-storage",
    "Region": "us-east-2"
  },
  "JWT": {
    "Secret": "your-secret-key-with-at-least-16-chars",
    "Issuer": "your-issuer",
    "Audience": "your-audience",
    "ExpiryInHours": 1,
    "RequireHttpsMetadata": true,
    "ValidateLifetime": true,
    "ValidateIssuerSigningKey": true
  },
  "Swagger": {
    "EnableAuthentication": false
  }
}